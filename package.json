  "description": "An extension of Testing Library focused on simulating keyboard-only users' behaviors",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "build": "tsc -p .",
    "test": "echo \"Error: no test specified\" && exit 1",
